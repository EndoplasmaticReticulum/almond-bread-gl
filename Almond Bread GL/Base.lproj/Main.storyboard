                                                        <label opaque="NO" userInteractionEnabled="NO" contentMode="left" horizontalHuggingPriority="251" verticalHuggingPriority="251" textAlignment="center" lineBreakMode="tailTruncation" numberOfLines="0" baselineAdjustment="alignBaselines" adjustsFontSizeToFit="NO" translatesAutoresizingMaskIntoConstraints="NO" id="h1A-vT-qim">
                                                            <rect key="frame" x="8" y="565" width="1008" height="29"/>
                                                            <string key="text">Benoit B. Mandelbrot (TED conference, 2010)
Picture by Steve Jurvetson</string>
                                                            <fontDescription key="fontDescription" type="italicSystem" pointSize="12"/>
