                                                        <button opaque="NO" contentMode="scaleToFill" contentHorizontalAlignment="center" contentVerticalAlignment="center" buttonType="roundedRect" lineBreakMode="middleTruncation" translatesAutoresizingMaskIntoConstraints="NO" id="ee0-As-hay">
                                                            <rect key="frame" x="483.5" y="594" width="57" height="27"/>
                                                            <fontDescription key="fontDescription" type="italicSystem" pointSize="12"/>
                                                            <state key="normal" title="CC BY 2.0"/>
                                                            <connections>
                                                                <action selector="licenseButtonTouchedWithSender:" destination="haN-Af-YW8" eventType="touchDown" id="HI6-Qh-WeB"/>
                                                            </connections>
                                                        </button>
                                                        <label opaque="NO" userInteractionEnabled="NO" contentMode="left" horizontalHuggingPriority="251" verticalHuggingPriority="251" textAlignment="justified" lineBreakMode="wordWrap" numberOfLines="0" baselineAdjustment="alignBaselines" adjustsFontSizeToFit="NO" translatesAutoresizingMaskIntoConstraints="NO" id="SAh-gS-8e2">
                                                            <rect key="frame" x="8" y="629" width="1008" height="41"/>
                                                            <string key="text">Mandelbrot is, among other accomplishments, well-known for his pioneering contributions to the field of geometry. The term 'fractal' describing self-similar structures dates back to him.</string>
                                                            <fontDescription key="fontDescription" type="system" pointSize="17"/>
